# - Config file for the tesseract package
# It defines the following variables
#  tesseract_INCLUDE_DIRS - include directories for tesseract
#  tesseract_LIBRARY_DIRS - library directories for tesseract
 
# Tell the user project where to find our headers and libraries
SET(tesseract_INCLUDE_DIRS "@tesseract_INCLUDE_DIRS@")
SET(tesseract_LIBRARY_DIRS "@tesseract_LIBRARY_DIRS@")
SET(tesseract_LIBRARY tesseract_static)
SET(tesseract_DATA_DIR "@tesseract_DATA_DIR@")

# Tell the user project where to find the tesseract use file
SET(tesseract_USE_FILE "@CMAKE_CURRENT_BINARY_DIR@/Usetesseract.cmake" )
